{
  "expo": {
    "name": "CristaliPOA",
    "description": "Aplicativo Cristali para vendedoras externas.",
    "slug": "CristaliPOA",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.6.21",
    "orientation": "portrait",
    "icon": "./assets/iconPOA.png",
    "splash": {
      "image": "./assets/splashPOA.png",
