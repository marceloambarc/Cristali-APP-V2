{
  "expo": {
    "name": "CristaliCWB",
    "description": "Aplicativo Cristali para vendedoras externas.",
    "slug": "CristaliCWB",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.6.2",
    "orientation": "portrait",
    "icon": "./assets/iconCWB.png",
    "splash": {
      "image": "./assets/splashCWB.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/notificationCWB.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "habilinfo.cristalicwb.app",
      "buildNumber": "1.0.9",
      "infoPlist": {
        "NSCameraUsageDescription": "This app do not uses the camera to let user put a photo in his profile page."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-iconCWB.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "package": "com.cristaliCWB.app",
      "versionCode": 20
    },
    "web": {
      "favicon": "./assets/faviconCWB.png"
    },
    "extra": {
      "eas": {
        "projectId": "0fbd4a70-c885-4a4c-b2dc-9b62a3e6dbeb"
      }
    }
  }
}
